# Local/private config goes in the include
[include]
        path = ~/.gitconfig.local
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = ~/.gitignore
        editor = vim
[mergetool]
        keepBackup = false
[help]
        autocorrect = 10
[push]
        default = current
[rebase]
        autosquash = true
[pull]
	ff = only
